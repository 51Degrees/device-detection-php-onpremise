            "url": "../FiftyOne.Pipeline.CloudRequestEngine",
            "options": {
                "symlink": true
            }
        },
        "dev-javascript": {
            "type": "path",
            "url": "../FiftyOne.Pipeline.JavaScriptBundler",
            "options": {
                "symlink": true
            }
        }
        
    },
    "require": {
        "51degrees/fiftyone.pipeline.core": "@dev",
        "51degrees/fiftyone.pipeline.engines": "@dev",
        "51degrees/fiftyone.pipeline.cloudrequestengine": "@dev",
